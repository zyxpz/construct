{
  "presets": [
      "es2015",
      "react",
      "stage-0",
      "env"
  ],
  "plugins": [
      [
          "transform-runtime",
          {
              "helpers": false,
              "polyfill": false,
              "regenerator": true,
              "moduleName": "babel-runtime"
          },
      ],
      "transform-vue-jsx"
  ]
}